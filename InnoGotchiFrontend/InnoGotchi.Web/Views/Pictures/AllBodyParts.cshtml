@using InnoGotchi.BLL.DTO;
@model IEnumerable<PictureDTO>

@foreach(var pic in Model)
{
    <div>
        <object width="100" height="100" type="image/svg+xml" data="data:image/svg+xml;base64,@(Convert.ToBase64String(pic.Image))">
            <img  src="data:image/png;base64,@(Convert.ToBase64String(pic.Image))" alt="Фолбэк">
        </object>
    </div>
}